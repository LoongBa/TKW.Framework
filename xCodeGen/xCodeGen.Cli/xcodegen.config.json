{
  "TargetProject": "src/MyProject/MyProject.csproj",
  "OutputRoot": "src/MyProject/Generated",
  "TemplatesPath": "Templates",
  "Debug": {
    "Enabled": true,
    "Directory": "_Debug"
  },
  "NamingRules": [
    {
      "ArtifactType": "Dto",
      "Pattern": "{Name}Dto"
    },
    {
      "ArtifactType": "Validator",
      "Pattern": "{Name}Validator"
    },
    {
      "ArtifactType": "ViewModel",
      "Pattern": "{Name}ViewModel"
    }
  ],
  "TemplateMappings": {
    "Dto": "Templates/Artifact/Dto.cshtml",
    "Validator": "Templates/Artifact/Validator.cshtml"
  },
  "OutputDirectories": {
    "Dto": "Dtos",
    "Validator": "Validators"
  },
  "EnabledUtilities": [
    "xCodeGen.Utilities.NamingUtility",
    "xCodeGen.Utilities.TypeUtility"
  ]
}